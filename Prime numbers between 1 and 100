 #include <iostream.h>
using namespace std;

int main()
{
    int count = 0,i,j;
    cout << "Prime numbers between 1 and 100 are: " << endl;
    for( i=2;i<=100; i++){
        for(j =1; j<=i; j++){
            if(i%j==0)
            count++;
       }
       if(count==2)
       cout<<i<<endl;
       count=0;
    }
  return 0;
}
